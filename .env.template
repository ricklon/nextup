# True Finals API Configuration
# Get these from your True Finals account settings
VITE_TF_USER_ID=
VITE_TF_API_KEY=

# OBS WebSocket Configuration
# Default OBS WebSocket server (modify if OBS is on a different machine/port)
VITE_OBS_URL=ws://localhost:4444
VITE_OBS_PASSWORD=

# Cloudflare Worker API URL
# This will be your deployed worker URL (set after deploying the Cloudflare Worker)
VITE_WORKER_URL=

# Default Tournament ID (optional)
# Can be overridden in the UI - use this to skip tournament selection
VITE_TOURNAMENT_ID=

# Cloudflare Configuration (for wrangler.toml)
# Account ID from your Cloudflare dashboard
# CLOUDFLARE_ACCOUNT_ID=
# D1 Database ID (created with: wrangler d1 create tournament-assignments)
# CLOUDFLARE_DATABASE_ID=
